import java.util.Scanner;

public class test1 {
		     public static void main(String args[]) {
		        Scanner s = new Scanner(System.in);
		        int a = s.nextInt(),b = s.nextInt();
		        if (a > b)
		        	System.out.print(a);
		        	System.out.print(b);
		        	else
		        		System.out.print(b);
		        		System.out.print(a);
		     }
	}