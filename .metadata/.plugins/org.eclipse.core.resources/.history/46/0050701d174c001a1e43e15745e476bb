import java.util.*;

public class test1 {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int f = 0;
		int t = s.nextInt();
		while (true) {
			t -= 1;
			if(t == 0) {
				break;
			}
			String l = s.next();
			if (l == "0") {
				f += 0;
			}
			if (l == "1") {
				f += 0;
			}
			if (l == "2") {
				f += 2;
			}
			if (l == "3") {
				f += 3;
			}
			if (l == "4") {
				f += 4;
			}
			if (l == "5") {
				f += 5;
			}
			if (l == "6") {
				f += 6;
			}
			if (l == "7") {
				f += 7;
			}
			if (l == "8") {
				f += 8;
			}
			if (l == "9") {
				f += 9;
			}
			if (l == "D") {
				f *= 2;
			}
			if (l == "C") {
				f = 0;
			}
}
		System.out.print(f);
	}
}